import java.util.Scanner;
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	 Scanner sc = new Scanner(System.in);
	 int t = sc.nextInt();
	 for(int i=0;i<t;i++)
	 {
	  int n = sc.nextInt();
	  int c=0,sum=0;
	  for(int j=1;sum<n;j++)
	  {
	   sum += j;
	   c++;
	   if((sum+j+1)>n && sum<n)
	   c--;
	  }
	  System.out.println(c);
	 }
	}
}
